<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Enable the possibility to view the encrypted data related with Payment Gateway Setting</description>
    <hasActivationRequired>true</hasActivationRequired>
    <label>Payment Gateway Setting Viewer</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>PaymentGatewaySetting__c</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ViewEncryptedData</name>
    </userPermissions>
</PermissionSet>
